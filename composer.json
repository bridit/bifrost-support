{
  "name": "bridit/bifrost-support",
  "description": "The Bifrost Support Package",
  "keywords": [
    "bridit",
    "laravel",
    "bifrost",
    "support"
  ],
  "homepage": "https://github.com/bridit/bifrost-support",
  "license": "MIT",
  "authors": [
    {
      "name": "Gustavo Siqueira",
      "email": "gus@brid-it.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Bifrost\\Support\\": ""
    },
    "files": [
      "helpers.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
